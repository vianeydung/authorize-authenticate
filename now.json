{
    "version": 2,
    "name": "authentication",
    "builds": [
        { "src": "www/package.json", "use": "@now/next" },
        { "src": "api/node/*.js", "use": "@now/node" }
    ],
    "routes": [
        { "src": "/api/(.*)", "dest": "/api/$1" },
        { "src": "/(.*)", "dest": "/www/$1" }
    ]
}